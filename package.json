{
	"name": "shopify-marketing-template",
	"version": "1.0.0",
	"description": "template for a Shopify project",
	"main": "index.js",
	"scripts": {
		"login:shopify": "echo \"Logging you out of Shopify CLI\" && shopify logout && dotenv -- cross-var shopify login --store=%SHOPIFY_URL%",
