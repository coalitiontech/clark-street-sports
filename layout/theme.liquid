<!-- navigation scripts -->
<script>
  $( document ).ready(function() {
    if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
      $('.tn-nav__item').on('click tap', function(e) {
        if (!$(this).hasClass('active')) {
          event.preventDefault();
          $('.tn-nav__item').removeClass('active');
          $(this).addClass('active');
        } 
      });
    } else {
      $('.tn-nav__item').hover(function() {
        if (!$(this).hasClass('active')) {
          event.preventDefault();
          $('.tn-nav__item').removeClass('active');
          $(this).addClass('active');
        }
      });
      $( "#shopify-section-navigation .megamenu" ).mouseleave(function() {
        $('.tn-nav__item').removeClass('active');
      });
    }
  });

  $(document).on('click touchstart', function (e) {
    if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
      var $tgt = $(e.target);
      if (!$tgt.closest("#shopify-section-navigation").length) {
        $('.tn-nav__item').removeClass('active');
      }
      if ($tgt.closest(".tn-nav__close").length) {
        $('.tn-nav__item').removeClass('active');
      }
    }
  });
</script>
    
