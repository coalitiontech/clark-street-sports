<script type="application/ld+json" id="eci-organization"></script>
<script type="application/ld+json" id="eci-breadcrumb"></script>
<script type="application/ld+json" id="eci-product"></script>
<script>
    $(function() {

        try{

            // organization
            var eci_organization = {
                data: {},
                prepare: function() {
                    this.data = {
                        "@context": "https://schema.org",
                        "@type": "Organization",
                        "url": "https://clarkstreetsports.com/",
                        "name": "Clark Street Sports"


                    };

                    // logo
                    this.data['logo'] = 'https://cdn.shopify.com/s/files/1/2242/6395/t/38/assets/logo@2x.png';

                    // contactPoint
                    var contact_sel = $('a[href^="tel:"]');
                    if( contact_sel.length ) {
                        this.data['contactPoint'] = [{
                            "@type": "ContactPoint",
                            "telephone": "+1-" + contact_sel.text().replace('(','').replace(') ','-'),
                            "contactType": "customer service"
                        }];
                    }else{
                        // console.log("no contact_sel");
                    }

                    // Social profile
                    var social_sel = $('div.socials').find('a');
                    if( social_sel.length ) {
                        this.data['sameAs'] = [];
                        if(social_sel.length > 1 ){
                            this.data['sameAs'].push($(social_sel[0]).attr('href'));
                            this.data['sameAs'].push($(social_sel[1]).attr('href'));
                            this.data['sameAs'].push($(social_sel[2]).attr('href'));
                            this.data['sameAs'].push($(social_sel[3]).attr('href'));
                        }
                    }else{
                        // console.log("no social_sel");
                    }

                },

                render: function() {
                    var obj = this;
                    $("#eci-organization").text(JSON.stringify(obj.data));
                    // console.log('eci-organization', JSON.stringify(obj.data));
                },

                init: function() {
                    // on home page only
                    if($('body.page-index').length){
                        this.prepare();
                        this.render();
                    }
                }
            };

            eci_organization.init();

            // breadcrumb
            var eci_breadcrumb = {
                data: {},
                prepare: function(breadcrumb) {

                    this.data = {
                        "@context": "https://schema.org",
                        "@type": "BreadcrumbList",
                        "itemListElement": []
                    }

                    var obj = this;
                    $(breadcrumb).each(function(i, s){
                        var item = {
                            "@type": "ListItem",
                            "position": i + 1,
                            "name": $(s).text().replace('>','')

                        };
                        item["item"] = 'https://clarkstreetsports.com' + $(s).attr('href');

                        obj.data.itemListElement.push(item);
                    });


                },
                render: function() {
                    var obj = this;
                    $("#eci-breadcrumb").text(JSON.stringify(obj.data));
                    // console.log('eci-breadcrumb', JSON.stringify(obj.data));
                },

                init: function() {

                    // wherever breadcrumb appears (category, product and cms page)
                    var breadcrumb = $('div.breadcrumbs').find('a');
                    if( breadcrumb.length ) {
                        this.prepare(breadcrumb);
                        this.render();
                    }else{
                        // console.log("no breadcrumb");
                    }

                }
            };

            eci_breadcrumb.init();

            // product
            var eci_product = {
                data: {},
                prepare: function () {

                    this.data = {
                        "@context": "https://schema.org/",
                        "@type": "Product",
                        "offers": {
                            "@type": "Offer",
                            "availability": "http://schema.org/" + eci_availability,
                            "priceCurrency": "USD",
                            "price": "{{ product.price | money_without_currency }}"
                        },
                        "brand" : {
                          "@type" : "Brand",
                          "name" : "Clark Street Sports"
                        }
                    };

                    //product sku
					var p_sku = $("div.product-description div.variant-sku").text();
                    if (p_sku.length){
                        this.data.sku = p_sku
                    } else {
                        console.log("no p_sku");
                    }
                  
                    //product name
                    var p_name = "{{ product.title }}"
                    if (p_name.length){
                        this.data.name = p_name
                    } else {
                        throw "no product_name";
                    }

                    //product image
                    var p_images = this.data.image = [];
                    if($("div.product-images .featured__images li").length){
                        $("div.product-images .featured__images li").each(function () {
                            p_images.push("https:" + $(this).find("img").attr('src'));
                        });
                        this.data.image = p_images;
                        // console.log('this.data.image', this.data.image);
                    } else {
                        throw "no product images"
                    }

                    //product description
                    var p_desc = "{{ product.description | strip_html }}";
                    if (p_desc.length){
                        this.data.description = p_desc
                    } else {
                        throw "no product description";
                    }

                    // url
                    this.data['offers']['url'] = window.location.href;
                },
                render: function () {
                    var obj = this;
                    $("#eci-product").text(JSON.stringify(obj.data));
                    //console.log('eci-product', JSON.stringify(obj.data));
                },
                init: function () {
                    //if ($("body.page-product").length) {
                        this.prepare();
                        this.render();
                    //}
                }
            };

            // update for targeting all product template 
            if($('.page-product').length){
                eci_product.init();
            }else {
                console.log('no product');
            }
          	//eci_product.init();
        }catch(e){

            console.log(e);
        }

    });
</script>
<!-- ECI:END SCHEMA, 11-22 -->
